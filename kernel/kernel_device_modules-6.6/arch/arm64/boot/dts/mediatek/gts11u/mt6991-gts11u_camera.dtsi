			slew-rate = <1>;
			output-low;
		};
	};
	camera_pins_cam0_afvdd_1: cam0@vcam5 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO186__FUNC_GPIO186>;
			slew-rate = <1>;
			output-high;
		};
	};

	/****************  Camera1  ***************/
	//MCLK
	camera_pins_cam1_mclk_off: camera-pins-cam1-mclk-off {
		pins-cmd-dat {
			pinmux = <PINMUX_GPIO210__FUNC_GPIO210>;
