/* FLASH */
&i2c5 {
	sm5714b@49 {
		sm5714b-fled {
			status = "okay";
			compatible = "siliconmitus,sm5714b-fled";
			flash-en-gpio = <&pio 70 0>;
			torch-en-gpio = <&pio 174 0>;

			flash-brightness = <0xB>; /* 0x0(300mA), 0x1(700mA), 0x2(800mA), 0x3(900mA) ~ 0xF(1500mA) : 50mA Step */
			preflash-brightness = <0x7>;
			torch-brightness = <0x7>; //0x0(50mA) ~ 0x7(225mA) : 25mA Step
			factory_current = <0x6>; //0x0(300mA)
			flashlight_current = <0 1 2 3 4>;
			timeout = <0xff>;
		};
	};
};

